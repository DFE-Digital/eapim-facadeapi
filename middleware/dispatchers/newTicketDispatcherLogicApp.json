                          "TicketUniqueIdentifier",
                          "OrganisationUniqueIdentifiers",
                          "ContactUniqueIdentifiers",
                          "AdvisorUniqueIdentifiers",
                          "AdvisorTeamUniqueIdentifiers",
                          "InteractionCommunicationChannel",
                          "InteractionDatetime",
                          "InteractionDirection",
                          "InteractionHeader",
                          "InteractionBody"
