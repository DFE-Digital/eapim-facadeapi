                        ],
                        "type": "object"
                      },
                      "type": "array"
                    },
                    "DatetimeTicketRaised": {
                      "type": "string"
                    },
                    "Interactions": {
                      "items": {
                        "properties": {
                          "AdvisorTeamUniqueIdentifiers": {
                            "items": {
                              "properties": {
                                "IdentifierType": {
                                  "type": "string"
                                },
                                "UniqueIdentifier": {
                                  "type": "string"
                                },
                                "UniqueIdentifierAlias": {
                                  "type": "string"
                                }
