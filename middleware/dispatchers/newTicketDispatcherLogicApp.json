                        },
                        "UniqueIdentifierAlias": {
                          "type": "string"
                        }
                      },
                      "type": "object"
                    }
                  },
                  "type": "object"
                }
              },
              "operationOptions": "EnableSchemaValidation"
            }
          },
          "actions": {
            "Response": {
              "runAfter": {
                "Send_message": [
                  "Succeeded"
                ]
              },
              "type": "Response",
              "kind": "Http",
              "inputs": {
                "body": "Accepted",
                "statusCode": 202
              }
            },
            "Send_message": {
              "runAfter": {},
              "type": "ApiConnection",
              "inputs": {
                "body": {
                  "ContentData": "@{base64(triggerBody())}"
                },
                "host": {
                  "connection": {
                    "name": "@parameters('$connections')['servicebus']['connectionId']"
                  }
                },
                "method": "post",
                "path": "/@{encodeURIComponent(encodeURIComponent(parameters('$connections')['servicebus']['queueName']))}/messages"
              }
            }
          },
          "outputs": {}
        },
        "parameters": {
          "$connections": {
            "value": {
              "servicebus": {
                "id": "[concat(subscription().id, '/providers/Microsoft.Web/locations/', 'westeurope', '/managedApis/', 'servicebus')]",
                "connectionId": "[resourceId('Microsoft.Web/connections', parameters('middleware_servicebus_connection_name'))]",
                "connectionName": "[parameters('middleware_servicebus_connection_name')]",
                "queueName": "[parameters('middleware_servicebus_queue_name')]"
              }
            }
          }
        }
      },
      "name": "[parameters('LogicAppName')]",
      "type": "Microsoft.Logic/workflows",
      "location": "[parameters('LogicAppLocation')]",
      "tags": {
        "Environment": "Dev",
        "Parent Business": "TBC",
        "Service Offering": "TBC",
        "displayName": "LogicApp"
      },
      "apiVersion": "2016-06-01",
      "dependsOn": [
        "[resourceId('Microsoft.Web/connections', parameters('middleware_servicebus_connection_name'))]"
      ]
    },
    {
      "type": "MICROSOFT.WEB/CONNECTIONS",
      "apiVersion": "2016-06-01",
      "name": "[parameters('middleware_servicebus_connection_name')]",
      "location": "westeurope",
      "properties": {
        "api": {
          "id": "[concat(subscription().id, '/providers/Microsoft.Web/locations/', 'westeurope', '/managedApis/', 'servicebus')]"
        },
        "displayName": "[parameters('middleware_servicebus_diaplay_name')]",
        "parameterValues": {
          "connectionString": "[parameters('middleware_servicebus_connection_string')]"
        }
      }
    }
  ],
  "outputs": {
    "logicAppResourceId": {
      "type": "string",
      "value": "[resourceId('Microsoft.Logic/workflows/', parameters('logicAppName'))]"
   }
  }
