                          "InteractionDatetime": {
                            "type": "string"
                          },
                          "InteractionDirection": {
                            "type": "string"
                          },
                          "InteractionHeader": {
                            "type": "string"
                          },
                          "InteractionSequenceNumber": {
                            "type": "string"
                          },
                          "InteractiontUniqueIdentifier": {
                            "properties": {
                              "IdentifierType": {
                                "type": "string"
                              },
                              "UniqueIdentifier": {
                                "type": "string"
                              },
                              "UniqueIdentifierAlias": {
                                "type": "string"
                              }
                            },
                            "type": "object"
                          },
                          "OrganisationUniqueIdentifiers": {
                            "items": {
                              "properties": {
                                "IdentifierType": {
                                  "type": "string"
                                },
                                "UniqueIdentifier": {
                                  "type": "string"
                                },
                                "UniqueIdentifierAlias": {
                                  "type": "string"
                                }
                              },
                              "required": [
                                "UniqueIdentifier",
                                "UniqueIdentifierAlias",
                                "IdentifierType"
                              ],
                              "type": "object"
