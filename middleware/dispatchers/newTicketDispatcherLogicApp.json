                                "IdentifierType": {
                                  "type": "string"
                                },
                                "UniqueIdentifier": {
                                  "type": "string"
                                },
                                "UniqueIdentifierAlias": {
                                  "type": "string"
                                }
                              },
                              "required": [
                                "UniqueIdentifier",
                                "UniqueIdentifierAlias",
                                "IdentifierType"
                              ],
                              "type": "object"
                            },
                            "type": "array"
                          },
                          "ContactUniqueIdentifiers": {
                            "items": {
                              "properties": {
                                "IdentifierType": {
                                  "type": "string"
                                },
                                "UniqueIdentifier": {
                                  "type": "string"
                                },
                                "UniqueIdentifierAlias": {
                                  "type": "string"
                                }
                              },
                              "required": [
                                "UniqueIdentifier",
                                "UniqueIdentifierAlias",
                                "IdentifierType"
                              ],
                              "type": "object"
                            },
                            "type": "array"
                          },
                          "InteractionBody": {
                            "type": "string"
                          },
                          "InteractionCommunicationChannel": {
