{
  "swagger": "2.0",
  "info": {
    "title": "Rosetta Stone API",
    "version": "1.0.0",
    "description": "Internal middleware component: records and describes relationships between data. A translation engine."
  },
  "basePath": "/v1/rosetta-stone",
  "paths": {
    "/reference-data-get-upload-json": {
      "post": {
        "description": "Returns a JSON payload showing JSON required to upload a reference data translation",
        "operationId": "reference-data-get-upload-json",
        "produces": [
          "application/json",
          "application/xml"
        ],
        "parameters": [
          {
            "in": "body",
            "name": "ReferenceDataUploadJsonParams",
            "description": "A JSON payload detailing the ElementTypeBK of the translation item",
            "required": true,
            "schema": {
              "$ref": "#/definitions/ReferenceDataUploadJsonParams"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "JSON payload showing JSON used to upload translations for the ElementTypeBK",
            "schema": {
              "$ref": "#/definitions/TranslationData"
            }
          },
          "400": {
            "description": "Bad Request - Error Message detailing what was wrong with the payload",
            "schema": {
              "type": "string"
            }
          },
          "404": {
            "description": "Not Found - Error Message detailing what was not found",
            "schema": {
              "type": "string"
            }
          },
          "500": {
            "description": "Internal System Error - stack trace in internal logs"
          }
        }
      }
    },
    "/reference-data-translate": {
      "post": {
        "description": "Translates a JSON payload source reference data item into its target value. Note: TargetSorBK can be 'ALL' to return all systems.",
        "operationId": "reference-data-translate",
        "produces": [
          "application/json",
          "application/xml"
        ],
        "parameters": [
          {
            "in": "body",
            "name": "ReferenceDataTranslateParams",
            "description": "A JSON payload detailing the source translation item",
            "required": true,
            "schema": {
              "$ref": "#/definitions/ReferenceDataTranslateParams"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "List of ReferenceDataTranslateResult items",
            "schema": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/ReferenceDataTranslateResult"
              }
            }
          },
          "400": {
            "description": "Bad Request - Error Message detailing what was wrong with the payload",
            "schema": {
              "type": "string"
            }
          },
          "404": {
            "description": "Not Found - Error Message detailing what was not found",
            "schema": {
              "type": "string"
            }
          },
          "500": {
            "description": "Internal System Error - stack trace in internal logs"
          }
        }
      }
    },
    "/reference-data-upload": {
      "post": {
        "description": "Transforms a JSON payload into reference data translations and inserts them into the Rosetta Stone database",
        "operationId": "reference-data-upload",
        "produces": [
          "application/json",
          "application/xml"
        ],
        "parameters": [
          {
            "in": "body",
            "name": "TranslationData",
            "description": "A JSON payload detailing the translations to insert",
            "required": true,
            "schema": {
              "$ref": "#/definitions/TranslationData"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "The reference data translations upload was a success"
          },
          "400": {
            "description": "Bad Request - Error Message detailing what was wrong with the payload",
            "schema": {
              "type": "string"
            }
          },
          "404": {
            "description": "Not Found - Error Message detailing what was not found",
            "schema": {
              "type": "string"
            }
          },
          "500": {
            "description": "Internal System Error - stack trace in internal logs"
          }
        }
      }
    },
    "/register-element-create": {
      "post": {
        "description": "Transforms a JSON payload into register elements and inserts them into the Rosetta Stone database",
        "operationId": "register-element-create",
        "produces": [
          "application/json",
          "application/xml"
        ],
        "parameters": [
          {
            "in": "body",
            "name": "RegisterElementCreateParams",
            "description": "A JSON payload detailing the register elements to create",
            "required": true,
            "schema": {
              "$ref": "#/definitions/RegisterElementCreateParams"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "The register elements upload was a success"
          },
          "400": {
            "description": "Bad Request - Error Message detailing what was wrong with the payload",
            "schema": {
              "type": "string"
            }
          },
          "404": {
            "description": "Not Found - Error Message detailing what was not found",
            "schema": {
              "type": "string"
            }
          },
          "500": {
            "description": "Internal System Error - stack trace in internal logs"
          }
        }
      }
    },
    "/register-search-by-element-type": {
      "post": {
        "description": "Transforms a JSON payload and searches the Rosetta Stone database for matching elements. returns a list of Sor unique identifiers",
        "operationId": "register-search-by-element-type",
        "produces": [
          "application/json",
          "application/xml"
        ],
        "parameters": [
          {
            "in": "body",
            "name": "RegisterSearchByElementTypeParams",
            "description": "A JSON payload detailing the register elements to search for",
            "required": true,
            "schema": {
              "$ref": "#/definitions/RegisterSearchByElementTypeParams"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "List of ElementMapResult objects",
            "schema": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/ElementMapResult"
              }
            }
          },
          "400": {
            "description": "Bad Request - Error Message detailing what was wrong with the payload",
            "schema": {
              "type": "string"
            }
          },
          "404": {
            "description": "Not Found - Error Message detailing what was not found",
            "schema": {
              "type": "string"
            }
          },
          "500": {
            "description": "Internal System Error - stack trace in internal logs"
          }
        }
      }
    },
    "/register-translate-sor-to-target": {
      "post": {
        "description": "Transforms a JSON payload and searches the Rosetta Stone database for matching matching Sors. Returns a list of Sor unique identifiers",
        "operationId": "register-translate-sor-to-target",
        "produces": [
          "application/json",
          "application/xml"
        ],
        "parameters": [
          {
            "in": "body",
            "name": "RegisterTranslateSorToTargetParams",
            "description": "A JSON payload detailing the Sor UID to search for",
            "required": true,
            "schema": {
              "$ref": "#/definitions/RegisterTranslateSorToTargetParams"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "List of TranslateSorResult objects",
            "schema": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/TranslateSorResult"
              }
            }
          },
          "400": {
            "description": "Bad Request - Error Message detailing what was wrong with the payload",
            "schema": {
              "type": "string"
            }
          },
          "404": {
            "description": "Not Found - Error Message detailing what was not found",
            "schema": {
              "type": "string"
            }
          },
          "500": {
            "description": "Internal System Error - stack trace in internal logs"
          }
        }
      }
    },
    "/system-of-reference-create": {
      "post": {
        "description": "Transforms a JSON payload and inserts or updates a System of Reference in the Rosetta Stone database",
        "operationId": "system-of-reference-create",
        "produces": [
          "application/json",
          "application/xml"
        ],
        "parameters": [
          {
            "in": "body",
            "name": "SORCreateParams",
            "description": "A JSON payload detailing the Sor to create",
            "required": true,
            "schema": {
              "$ref": "#/definitions/SORCreateParams"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "The Sor upload was a success"
          },
          "400": {
            "description": "Bad Request - Error Message detailing what was wrong with the payload",
            "schema": {
              "type": "string"
            }
          },
          "404": {
            "description": "Not Found - Error Message detailing what was not found",
            "schema": {
              "type": "string"
            }
          },
          "500": {
            "description": "Internal System Error - stack trace in internal logs"
          }
        }
      }
    },
    "/heart-beat": {
      "get": {
        "description": "Standard diagnostics method, used to test the health of the facade's underlying system.",
        "operationId": "heart-beat",
        "parameters": [],
        "responses": {
          "200": {
            "description": "The standard/only response from the method."
          }
        }
      }
    }
  },
  "securityDefinitions": {
    "authKey": {
      "type": "apiKey",
      "name": "x-functions-key",
      "in": "header"
    }
  },
  "definitions": {
    "ReferenceDataUploadJsonParams": {
      "type": "object",
      "required": [
        "ElementTypeBK"
      ],
      "properties": {
        "ElementTypeBK": {
          "type": "string",
          "example": "TicStat"
        }
      }
    },
    "ReferenceDataTranslateParams": {
      "type": "object",
      "required": [
        "SourceSorBK",
        "SourceEntityBK",
        "SourceElementTypeBK",
        "SourceElementValue",
        "TargetSorBK"
      ],
      "properties": {
        "SourceSorBK": {
          "type": "string",
          "example": "HD"
        },
        "SourceEntityBK": {
          "type": "string",
          "example": "ACC"
        },
        "SourceElementTypeBK": {
          "type": "string",
          "example": "CHNo"
        },
        "SourceElementValue": {
          "type": "string",
          "example": "A12345"
        },
        "TargetSorBK": {
          "type": "string",
          "example": "SCRM"
        }
      }
    },
    "ReferenceDataTranslateResult": {
      "type": "object",
      "required": [
        "SorBK",
        "SorName",
        "EntityBK",
        "EntityName",
        "ElementTypeBK",
        "ElementTypeName",
        "ElementValue",
        "ElementDescription"
      ],
      "properties": {
        "SorBK": {
          "type": "string",
          "example": "HD"
        },
        "SorName": {
          "type": "string",
          "example": "Helpdesk"
        },
        "EntityBK": {
          "type": "string",
          "example": "ACC"
        },
        "EntityName": {
          "type": "string",
          "example": "Account"
        },
        "ElementTypeBK": {
          "type": "string",
          "example": "AccStat"
        },
        "ElementTypeName": {
          "type": "string",
          "example": "Account Status"
        },
        "ElementValue": {
          "type": "string",
          "example": "1"
        },
        "ElementDescription": {
          "type": "string",
          "example": "Open"
        }
      }
    },
    "TranslationData": {
      "type": "object",
      "required": [
        "Translations"
      ],
      "properties": {
        "Translations": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Translation"
          }
        }
      }
    },
    "Translation": {
      "type": "object",
      "required": [
        "ElementTypeBK",
        "ElementTypeName",
        "ReferenceSets"
      ],
      "properties": {
        "ElementTypeBK": {
          "type": "string",
          "example": "AccStat"
        },
        "ElementTypeName": {
          "type": "string",
          "example": "Account Status"
        },
        "ReferenceSets": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Referenceset"
          }
        }
      }
    },
    "Referenceset": {
      "type": "object",
      "required": [
        "ElementDetails"
      ],
      "properties": {
        "ElementDetails": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Elementdetail"
          }
        }
      }
    },
    "Elementdetail": {
      "type": "object",
      "required": [
        "SorBK",
        "EntityBK",
        "ElementValue",
        "ElementDescription"
      ],
      "properties": {
        "SorBK": {
          "type": "string",
          "example": "HD"
        },
        "EntityBK": {
          "type": "string",
          "example": "ACC"
        },
        "ElementValue": {
          "type": "string",
          "example": "1"
        },
        "ElementDescription": {
          "type": "string",
          "example": "Open"
        }
      }
    },
    "RegisterElementCreateParams": {
      "type": "object",
      "required": [
        "SourceSorBK",
        "EntityBK",
        "SourceSorUIDVal",
        "SourceDataElements"
      ],
      "properties": {
        "SourceSorBK": {
          "type": "string",
          "example": "HD"
        },
        "EntityBK": {
          "type": "string",
          "example": "CHNo"
        },
        "TargetSorBK": {
          "type": "string",
          "example": "SCRM"
        },
        "SourceSorUIDVal": {
          "type": "string",
          "example": "A12345"
        },
        "TargetSorUIDVal": {
          "type": "string",
          "example": "B180C301-B289.."
        },
        "SourceDataElements": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/SourceDataElement"
          }
        }
      }
    },
    "SourceDataElement": {
      "type": "object",
      "required": [
        "ElementTypeBK",
        "ElementValue"
      ],
      "properties": {
        "ElementTypeBK": {
          "type": "string",
          "example": "CHNo"
        },
        "ElementValue": {
          "type": "string",
          "example": "A123456"
        }
      }
    },
    "RegisterSearchByElementTypeParams": {
      "type": "object",
      "required": [
        "SearchElementTypeBK",
        "SearchElementValue"
      ],
      "properties": {
        "SearchElementTypeBK": {
          "type": "string",
          "example": "CHNo"
        },
        "SearchElementValue": {
          "type": "string",
          "example": "A123456"
        },
        "SearchSorBK": {
          "type": "string",
          "example": "HD"
        },
        "SearchEntityBK": {
          "type": "string",
          "example": "ACC"
        }
      }
    },
    "ElementMapResult": {
      "type": "object",
      "required": [
        "SorBK",
        "SorUIDVal"
      ],
      "properties": {
        "SorBK": {
          "type": "string",
          "example": "HD"
        },
        "SorUIDVal": {
          "type": "string",
          "example": "A123456"
        }
      }
    },
    "RegisterTranslateSorToTargetParams": {
      "type": "object",
      "required": [
        "SourceSorBK",
        "SourceEntityBK",
        "SourceSorUIDVal",
        "TargetSorBK"
      ],
      "properties": {
        "SourceSorBK": {
          "type": "string",
          "example": "HD"
        },
        "SourceEntityBK": {
          "type": "string",
          "example": "ACC"
        },
        "SourceSorUIDVal": {
          "type": "string",
          "example": "A123456"
        },
        "TargetSorBK": {
          "type": "string",
          "example": "SCRM"
        }
      }
    },
    "TranslateSorResult": {
      "type": "object",
      "required": [
        "SourceSorBK",
        "SourceEntityBK",
        "SourceSorUIDVal",
        "TargetSorBK",
        "TargetSorUIDVal"
      ],
      "properties": {
        "SourceSorBK": {
          "type": "string",
          "example": "HD"
        },
        "SourceEntityBK": {
          "type": "string",
          "example": "ACC"
        },
        "SourceSorUIDVal": {
          "type": "string",
          "example": "A123456"
        },
        "TargetSorBK": {
          "type": "string",
          "example": "SCRM"
        },
        "TargetSorUIDVal": {
          "type": "string",
          "example": "B180C301-B289.."
        }
      }
    },
    "SORCreateParams": {
      "type": "object",
      "required": [
        "SorBK",
        "SorName",
        "InternalSor"
      ],
      "properties": {
        "SorBK": {
          "type": "string",
          "example": "HD"
        },
        "SorName": {
          "type": "string",
          "example": "Helpdesk"
        },
        "InternalSor": {
          "type": "boolean",
          "example": true
        }
      }
    }
  }
}
